  <DataSets>
    <DataSet Name="PlayerSubDataSet">
      <Query>
        <DataSourceName>PlayerDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CountryName">
            <Value>=Parameters!CountryName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        PlayerName, MatchPlayed, RunsMade, WicketsTaken, FeePerMatch
FROM            tbl_Players
WHERE        (BelongsTo = @CountryName)</CommandText>
      </Query>
